<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.craffic.convey.jobserver.dao.SysProvinceMapper">

    <resultMap id="BaseResultMap" type="com.craffic.convey.jobserver.model.SysProvince">
        <id property="provinceId" column="PROVINCE_ID" jdbcType="NUMERIC"></id>
        <result property="provinceName" column="PROVINCE_NAME" jdbcType="VARCHAR"></result>
    </resultMap>

    <sql id="BaseColumns">
        PROVINCE_ID, PROVINCE_NAME
    </sql>

    <select id="getAllProvinceList" resultMap="BaseResultMap">
        SELECT
        <include refid="BaseColumns" />
          FROM SYS_PROVINCE
    </select>
</mapper>

